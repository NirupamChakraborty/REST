<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table, tr,td{
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
    <%   for(result of result){%>
        <tr>
            <td><%= result.id %> </td>
            <td><%= result.username%> </td>
            <td><%= result.email%> </td>
            <td>
            <form method="GET" action="/user/<%=result.id %>/edit">
                <button>
                    edit username
                </button>
            </form>
        </td>
        </tr>
    <% }  %>
    </table>
</body>
</html>

